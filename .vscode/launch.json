  "version": "0.2.0",
  "configurations": [
    {
      "name": "preview",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "preview"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "env": {
        "DEV_SERVER_HTTP_OVERRIDES": "1"
      },
    },
    {
      "name": "build",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "build"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "command": "npm run dev",
      "name": "dev",
      "request": "launch",
      "type": "node-terminal",
      "env": {
        "DEV_SERVER_HTTP_OVERRIDES": "1"
      },
    }
  ]
}